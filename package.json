{
  "name": "renshu",
  "version": "1.0.0",
  "description": "Taiko practice tracker",
  "author": "Michael Bayne (http://github.com/samskivert)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samskivert/renshu.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test.*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "firebase": "^9.6.0",
    "firebaseui": "^6.0.0",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "mobx-react-devtools": "^6.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-firebaseui": "^6.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "jest": "^27.4.3",
    "ts-jest": "^27.1.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  }
}
