{
  "name": "renshu",
  "version": "1.0.0",
  "description": "Taiko practice tracker",
  "author": "Michael Bayne (http://github.com/samskivert)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samskivert/renshu.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test.*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "firebase": "^5.9.0",
    "firebaseui": "^3.5.2",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-firebaseui": "^3.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.6",
    "@firebase/util": "^0.2.10",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "flow-bin": "^0.95.1",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.4",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.4"
  }
}
